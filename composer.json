{
  "name": "contributte/console",
  "description": "Best Symfony Console for Nette Framework",
  "keywords": [
    "nette",
    "symfony",
    "console"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/console",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=7.2",
    "contributte/di": "^0.5.1",
    "symfony/console": "^4.2.9|^5.0.0"
  },
  "require-dev": {
    "contributte/qa": "^0.1.0",
    "nette/http": "~3.0.1",
    "phpstan/phpstan": "^1.0",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "phpstan/phpstan-nette": "^1.0",
    "phpstan/phpstan-strict-rules": "^1.0",
    "symfony/event-dispatcher": "^4.3.0 || ^5.0.0"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Console\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Cases\\": "tests/cases",
      "Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.10.x-dev"
    }
  }
}
